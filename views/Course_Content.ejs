
<%- include("layouts/header") %>
<div class="main_con">
            <div class="sidenav">
                
                <h2 class="Name"><%= course_title %></h2>

                <div class="choices">
                    <% for(let i = 0; i < videos.length; i++){ %>
                    <a href="#" x = "<%= videos[i].Video_Link %>" id = "<% videos[i].Video_Name %>"><%= videos[i].Video_Name %></a>
                    <hr style="border-top: 1px solid #bbb;">
                    <% } %>
                </div>
            </div>

            <div class = "container" style="height: 600px">
                <div class = "video-container" style="height: 100%;">
                    <iframe class="video" width="100%" height="100%" src="//www.youtube.com/embed/uzkc-qNVoOk" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                </div>

            </div>
        </div>
</div>
 <%- include("layouts/footer") %>
